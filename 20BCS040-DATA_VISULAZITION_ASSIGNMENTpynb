        "## choropleth maps"
      ],
      "metadata": {
        "id": "sLT1m113tNFl"
      }
    },
    {
      "cell_type": "code",
      "source": [
        "custom_purple_color_scale = [\n",
        "    [0.0, 'rgb(128, 0, 128)'],     # Dark Purple\n",
        "    [0.2, 'rgb(160, 32, 240)'],    # Slightly lighter purple\n",
        "    [0.4, 'rgb(192, 0, 255)'],     # Bright purple\n",
        "    [0.6, 'rgb(200, 100, 255)'],   # Light purple\n",
